  "specialTraits": {
    "blocks": [
      {
        "name": "Consume Life",
        "text": "As a bonus action, [name] can target one creature it can see within 5 ft. of it that has 0 hit points and is still alive. The target must succeed on a DC 10 Constitution saving throw against this magic or die. If the target dies, [name] regains DMG[3d6] hit points."
      },
      {
        "name": "Ephemeral",
        "text": "[name] can't wear or carry anything."
      },
      {
        "name": "Incorporeal Movement",
        "text": "[name] can move through other creatures and objects as if they were difficult terrain. It takes DMG[1d10] force damage if it ends its turn inside an object."
      },
      {
        "name": "Variable Illumination",
        "text": "[name] sheds bright light in a 5- to 20-foot radius and dim light for an additional number of ft. equal to the chosen radius. [name] can alter the radius as a bonus action."
      }
    ]
  },
  "actions": {
    "blocks": [
      {
        "name": "Shock",
        "text": "*Melee Spell Attack:* +4 to hit, reach 5 ft., one creature. *Hit:* DMG[2d8] lightning damage."
      },
      {
        "name": "Invisibility",
        "text": "[name] and its light magically become invisible until it attacks or uses its Consume Life, or until its concentration ends (as if concentrating on a spell)."
