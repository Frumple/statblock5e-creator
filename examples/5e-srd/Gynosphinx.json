      {
        "name": "Multiattack",
        "text": "[name] makes two claw attacks."
      },
      {
        "name": "Claw",
        "text": "*Melee Weapon Attack:* atk[str] to hit, reach 5 ft., one target. *Hit:* dmg[2d8 + str] slashing damage."
