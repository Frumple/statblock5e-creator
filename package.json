{
  "name": "statblock5e",
  "version": "1.0.0",
  "description": "A Web Component statblock for D&D 5E\r ====================================",
  "main": "index.js",
  "dependencies": {
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@testing-library/jest-dom": "^4.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "jest": "^24.9.0"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/js/init.js",
      "!src/js/parsers/scripts/generate-parser-script.js"
    ],
    "moduleNameMapper": {
      "^/(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "<rootDir>/src/js/helpers/test/expect-matchers",
      "<rootDir>/src/js/helpers/test/test-globals"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frumple/statblock5e.git"
  },
